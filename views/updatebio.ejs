<%- include('include/_header') %>  
    <div class="container my-2" style="width: 300px;">
      <h2 class="text-center my-2">Update Biodata</h2>
      <form method="POST" action="/api/biodata" id="userBio">
        <div class="form-group">
          <input type="text" class="form-control" name="fullname" placeholder="Fullname">
        </div>
        <div class="form-group">
          <input type="text" class="form-control" name="email" placeholder="Email">
        </div>
        <button type="submit" class="btn btn-primary text-center" id="submitBio">Save</button>
      </form>
    </div>         
<%- include('include/_footer') %>